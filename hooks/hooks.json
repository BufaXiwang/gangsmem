{
  "SessionEnd": [
    {
      "matcher": "*",
      "hooks": [
        {
          "type": "command",
          "command": "python3 \"${PLUGIN_DIR}/hooks/session_end.py\"",
          "timeout": 30000
        }
      ]
    }
  ],
  "UserPromptSubmit": [
    {
      "matcher": "*",
      "hooks": [
        {
          "type": "command",
          "command": "python3 \"${PLUGIN_DIR}/hooks/inject_memory.py\"",
          "timeout": 5000
        }
      ]
    }
  ]
}
